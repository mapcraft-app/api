{
  "name": "mapcraft-api",
  "description": "Official API for developping your own plugin for Mapcraft ecosystem",
  "author": "ClÃ©ment Bertrand (https://github.com/c-bertran)",
  "license": "GPL-3.0",
  "homepage": "https://mapcraft.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mapcraft-app/api"
  },
  "keywords": [
    "mapcraft",
    "minecraft",
    "api",
    "mapcraft-api"
  ],
  "version": "2.3.3",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/frontend.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/frontend.d.ts"
    },
    "./backend": {
      "import": "./dist/backend.es.js",
      "require": "./dist/backend.cjs.js",
      "types": "./dist/types/src/backend.d.ts"
    },
    "./frontend": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/frontend.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "backend": [
        "dist/backend.d.ts"
      ],
      "frontend": [
        "dist/frontend.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">= 18.0"
  },
  "bin": {
    "mapcraft": "./dist/cli.cjs.js"
  },
  "scripts": {
    "mapcraft": "node ./dist/cli.cjs.js",
    "build": "rollup --bundleConfigAsCjs --config rollup.config.js",
    "build:dev": "SET DEV=true&& rollup --bundleConfigAsCjs --config rollup.config.js",
    "watch": "rollup -w --bundleConfigAsCjs --config rollup.config.js ",
    "dev": "SET DEV=true&& node .\\dist\\index.cjs.js",
    "lint": "eslint -c ./.eslintrc.js --fix ."
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "better-sqlite3": "^9.4.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "rollup": "^4.13.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-plugin-ts-paths": "^1.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  }
}
