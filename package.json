{
  "name": "mapcraft-api",
  "description": "Official API for developping your own plugin for Mapcraft ecosystem",
  "author": "ClÃ©ment Bertrand (https://github.com/c-bertran)",
  "license": "GPL-3.0",
  "homepage": "https://mapcraft.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mapcraft-app/api"
  },
  "keywords": [
    "mapcraft",
    "minecraft",
    "api",
    "mapcraft-api"
  ],
  "version": "2.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./backend": {
      "import": "./dist/backend.es.js",
      "require": "./dist/backend.cjs.js",
      "types": "./dist/types/src/backend.d.ts"
    },
    "./frontend": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "backend": [
        "dist/types/src/backend.d.ts"
      ],
      "frontend": [
        "dist/types/src/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">= 16.0"
  },
  "bin": {
    "mapcraft": "./dist/cli.cjs.js"
  },
  "scripts": {
    "mapcraft": "node ./dist/cli.js",
    "build": "rollup --bundleConfigAsCjs --config rollup.config.js",
    "watch": "rollup -w --bundleConfigAsCjs --config rollup.config.js ",
    "dev": "SET DEV=true&& node .\\dist\\module.cjs.js"
  },
  "dependencies": {
    "7zip-min": "^1.4.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "rollup": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-ts-paths": "^1.0.5",
    "typescript": "^4.8.4"
  }
}
