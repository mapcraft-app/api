{
  "name": "mapcraft-api",
  "version": "1.7.0",
  "description": "Official API for developping your own plugin for Mapcraft ecosystem",
  "main": "index.js",
  "bin": {
    "mapcraft-api": "cli.js"
  },
  "out": "out",
  "engines": {
    "node": ">= 8.6"
  },
  "types": "declaration/index.d.ts",
  "files": [
    "declaration/*",
    "modules/**/*",
    "cli/**/*",
    "index.d.ts",
    "index.js",
    "models.js",
    "yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cbertran/mapcraft-api.git"
  },
  "keywords": [
    "mapcraft",
    "minecraft"
  ],
  "author": "Cl√©ment Bertrand (https://gitlab.com/cbertran/mapcraft-api)",
  "license": "GPL-3.0",
  "homepage": "https://mapcraft.app",
  "scripts": {
    "test": "node cli.js",
    "lint": "eslint .",
    "declar": "tsc",
    "rebuild": "./node_modules/.bin/electron-rebuild.cmd"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "better-sqlite3": "^7.5.0"
  },
  "devDependencies": {
    "electron": "^16.0.8",
    "electron-rebuild": "^3.2.7",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "node-gyp": "^8.4.1",
    "typescript": "^4.5.5",
    "inquirer": "^8.2.0"
  }
}
